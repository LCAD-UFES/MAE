int num_char_train;
int num_char_test;
int i;

num_char_train = 60000;
num_char_test = 10000;

destroy network;
reload from "netmem2";

SetNetworkStatus(1); # Test data set
i = 0;
move character to 4, 17; /* volta um para comeccar do zero */
while (i < num_char_test)
{
	move character to 20, 14; /* avancca um */
	forward network;
	if (EvaluateChar(0.5) == 0)
		EvaluateCharInRangePixels(1);

	UpdateStats(0);
	i = i + 1;
}

quit;
