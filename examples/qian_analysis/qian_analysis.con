#params: gabor_filter ("%f %f %f %f %f %d %d", <sigma_x>, <sigma_y>, <phase_x>, <cycles_x>, <theta>, <kernel_size>, <mapping>)

# Kernels
filter 		stimulus_left 	   with gabor_filter ("%f %f %f %f %f %d %d", 2.7, 2.3, -pi/2.0, 1.5, 0.0, BIG_KERNEL, MAP_PLAN) producing big_kernel_left_1;
filter 		stimulus_left 	   with gabor_filter ("%f %f %f %f %f %d %d", 2.7, 2.3, -pi/4.0, 1.5, 0.0, BIG_KERNEL, MAP_PLAN) producing big_kernel_left_2;
filter 		stimulus_left 	   with gabor_filter ("%f %f %f %f %f %d %d", 2.7, 2.3,     0.0, 1.5, 0.0, BIG_KERNEL, MAP_PLAN) producing big_kernel_left_3;
filter 		stimulus_left 	   with gabor_filter ("%f %f %f %f %f %d %d", 2.7, 2.3,  pi/4.0, 1.5, 0.0, BIG_KERNEL, MAP_PLAN) producing big_kernel_left_4;
filter 		stimulus_left 	   with gabor_filter ("%f %f %f %f %f %d %d", 2.7, 2.3,  pi/2.0, 1.5, 0.0, BIG_KERNEL, MAP_PLAN) producing big_kernel_left_5;
filter 		stimulus_left 	   with gabor_filter ("%f %f %f %f %f %d %d", 2.7, 2.3, 0.75*pi, 1.5, 0.0, BIG_KERNEL, MAP_PLAN) producing big_kernel_left_6;
filter 		stimulus_left 	   with gabor_filter ("%f %f %f %f %f %d %d", 2.7, 2.3,      pi, 1.5, 0.0, BIG_KERNEL, MAP_PLAN) producing big_kernel_left_7;

