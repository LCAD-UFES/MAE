filter 		stimulus_right     with gabor_filter ("%f %f %f %f %f %d %d", 2.7, 2.3,  pi/4.0, 1.5, 0.0, MEDIUM_KERNEL, MAP_PLAN) producing medium_kernel_right_4;
filter 		stimulus_right     with gabor_filter ("%f %f %f %f %f %d %d", 2.7, 2.3,  pi/2.0, 1.5, 0.0, MEDIUM_KERNEL, MAP_PLAN) producing medium_kernel_right_5;
filter 		stimulus_right     with gabor_filter ("%f %f %f %f %f %d %d", 2.7, 2.3, 0.75*pi, 1.5, 0.0, MEDIUM_KERNEL, MAP_PLAN) producing medium_kernel_right_6;
filter 		stimulus_right     with gabor_filter ("%f %f %f %f %f %d %d", 2.7, 2.3,      pi, 1.5, 0.0, MEDIUM_KERNEL, MAP_PLAN) producing medium_kernel_right_7;

filter 		stimulus_left 	   with gabor_filter ("%f %f %f %f %f %d %d", 2.3, 2.3, -pi/2.0, 1.5, 0.0, SMALL_KERNEL, MAP_PLAN) producing small_kernel_left_1;
filter 		stimulus_left 	   with gabor_filter ("%f %f %f %f %f %d %d", 2.3, 2.3, -pi/4.0, 1.5, 0.0, SMALL_KERNEL, MAP_PLAN) producing small_kernel_left_2;
filter 		stimulus_left 	   with gabor_filter ("%f %f %f %f %f %d %d", 2.3, 2.3,     0.0, 1.5, 0.0, SMALL_KERNEL, MAP_PLAN) producing small_kernel_left_3;
filter 		stimulus_left 	   with gabor_filter ("%f %f %f %f %f %d %d", 2.3, 2.3,  pi/4.0, 1.5, 0.0, SMALL_KERNEL, MAP_PLAN) producing small_kernel_left_4;
filter 		stimulus_left 	   with gabor_filter ("%f %f %f %f %f %d %d", 2.3, 2.3,  pi/2.0, 1.5, 0.0, SMALL_KERNEL, MAP_PLAN) producing small_kernel_left_5;
filter 		stimulus_left 	   with gabor_filter ("%f %f %f %f %f %d %d", 2.3, 2.3, 0.75*pi, 1.5, 0.0, SMALL_KERNEL, MAP_PLAN) producing small_kernel_left_6;
filter 		stimulus_left 	   with gabor_filter ("%f %f %f %f %f %d %d", 2.3, 2.3,      pi, 1.5, 0.0, SMALL_KERNEL, MAP_PLAN) producing small_kernel_left_7;

filter 		stimulus_right     with gabor_filter ("%f %f %f %f %f %d %d", 2.3, 2.3, -pi/2.0, 1.5, 0.0, SMALL_KERNEL, MAP_PLAN) producing small_kernel_right_1;
filter 		stimulus_right     with gabor_filter ("%f %f %f %f %f %d %d", 2.3, 2.3, -pi/4.0, 1.5, 0.0, SMALL_KERNEL, MAP_PLAN) producing small_kernel_right_2;
filter 		stimulus_right     with gabor_filter ("%f %f %f %f %f %d %d", 2.3, 2.3,     0.0, 1.5, 0.0, SMALL_KERNEL, MAP_PLAN) producing small_kernel_right_3;
filter 		stimulus_right     with gabor_filter ("%f %f %f %f %f %d %d", 2.3, 2.3,  pi/4.0, 1.5, 0.0, SMALL_KERNEL, MAP_PLAN) producing small_kernel_right_4;
filter 		stimulus_right     with gabor_filter ("%f %f %f %f %f %d %d", 2.3, 2.3,  pi/2.0, 1.5, 0.0, SMALL_KERNEL, MAP_PLAN) producing small_kernel_right_5;
filter 		stimulus_right     with gabor_filter ("%f %f %f %f %f %d %d", 2.3, 2.3, 0.75*pi, 1.5, 0.0, SMALL_KERNEL, MAP_PLAN) producing small_kernel_right_6;
filter 		stimulus_right     with gabor_filter ("%f %f %f %f %f %d %d", 2.3, 2.3,      pi, 1.5, 0.0, SMALL_KERNEL, MAP_PLAN) producing small_kernel_right_7;

# Simple cells
