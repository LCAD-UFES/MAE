# Graphs
filter		big_simple_cell_1, big_simple_cell_2, big_simple_cell_3, big_simple_cell_4, big_simple_cell_5 
		with graph_filter ("%f %f %f %f", -pi/2.0, pi/2.0, -50.0, 50.0) producing graph_big_simple_cell;		
filter		medium_simple_cell_1, medium_simple_cell_2, medium_simple_cell_3, medium_simple_cell_4, medium_simple_cell_5 
		with graph_filter ("%f %f %f %f", -pi/2.0, pi/2.0, -50.0, 50.0) producing graph_medium_simple_cell;		
filter		small_simple_cell_1, small_simple_cell_2, small_simple_cell_3, small_simple_cell_4, small_simple_cell_5
		with graph_filter ("%f %f %f %f", -pi/2.0, pi/2.0, -50.0, 50.0) producing graph_small_simple_cell;
		
filter		big_complex_cell_1, big_complex_cell_2, big_complex_cell_3, big_complex_cell_4, big_complex_cell_5 
		with graph_filter ("%f %f %f %f", -pi/2.0, pi/2.0, 0.0, 2000.0) producing graph_big_complex_cell;
