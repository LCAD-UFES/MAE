filter 		stimulus_right 	   with gabor_filter ("%f %f %f %f %f %d %d", 2.7, 2.3,      pi, 1.5, 0.0, BIG_KERNEL, MAP_PLAN) producing big_kernel_right_7;

filter 		stimulus_left 	   with gabor_filter ("%f %f %f %f %f %d %d", 2.7, 2.3, -pi/2.0, 1.5, 0.0, MEDIUM_KERNEL, MAP_PLAN) producing medium_kernel_left_1;
filter 		stimulus_left 	   with gabor_filter ("%f %f %f %f %f %d %d", 2.7, 2.3, -pi/4.0, 1.5, 0.0, MEDIUM_KERNEL, MAP_PLAN) producing medium_kernel_left_2;
filter 		stimulus_left 	   with gabor_filter ("%f %f %f %f %f %d %d", 2.7, 2.3,     0.0, 1.5, 0.0, MEDIUM_KERNEL, MAP_PLAN) producing medium_kernel_left_3;
