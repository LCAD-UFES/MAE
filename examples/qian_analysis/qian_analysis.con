		with graph_filter ("%f %f %f %f", -pi/2.0, pi/2.0, 0.0, 2000.0) producing graph_medium_complex_cell;
filter		small_complex_cell_1, small_complex_cell_2, small_complex_cell_3, small_complex_cell_4, small_complex_cell_5
		with graph_filter ("%f %f %f %f", -pi/2.0, pi/2.0, 0.0, 2000.0) producing graph_small_complex_cell;
		
################################## Connect Neuron Layers to Outputs #############################################

outputconnect	graph_big_simple_cell 			to graph_big_simple_cell_out;
outputconnect	graph_medium_simple_cell 		to graph_medium_simple_cell_out;
outputconnect	graph_small_simple_cell 		to graph_small_simple_cell_out;

outputconnect	graph_big_complex_cell 			to graph_big_complex_cell_out;
outputconnect	graph_medium_complex_cell 		to graph_medium_complex_cell_out;
outputconnect	graph_small_complex_cell 		to graph_small_complex_cell_out;
